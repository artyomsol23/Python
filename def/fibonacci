def fibonacci_1(n):  # Рекурсивный вариант
    if n <= 1:
        return n
    return fib(n - 1) + fib(n - 2)

def fibonacci_2(n):  # Индуктивный (итеративный) вариант
    a, b = 0, 1
    for _ in range(n):
        a, b = b, a + b
    return a

print("5-е число Фибоначчи:", fibonacci_1(5))  # 5
print("5-е число Фибоначчи:", fibonacci_1(5))  # 5
